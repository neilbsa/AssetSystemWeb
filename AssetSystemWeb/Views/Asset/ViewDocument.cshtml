@model SystemEntities.Models.AssetHeaderFiles


@{
    Layout = null;
}


@if (Model.contentLenght > 0)
{
    if (Model.contentType == "application/pdf")
    {
        <EMBED style="width:100%;height:100%;min-height:700px" Src="@String.Format("data:{1};base64,{0}",Model.content64base,Model.contentType)">
    }
    else if (Model.contentType.Contains("image"))
    {
        <EMBED style="width:100%;height:100%;" Src="@String.Format("data:{1};base64,{0}",Model.content64base,Model.contentType)">

    }
    else
    {
        <span>No view available</span>
    }

}
else
{

    <span>No view available</span>
}
