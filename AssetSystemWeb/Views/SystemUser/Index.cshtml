@model IEnumerable<SystemEntities.Models.UserProfile>
@{ 
    ViewBag.Title = "Index";
}


<div class="modal fade" id="userModel" tabindex="-1" role="dialog" aria-labelledby="userModel-label">
    <div class="modal-dialog" role="document">
        <div class="modal-content">
            <div class="modal-header">
                <button type="button" class="close" data-dismiss="modal" aria-label="Close"><span aria-hidden="true">&times;</span></button>
                <h4 class="modal-title" id="userModel-label"><span></span></h4>
            </div>
            <div class="modal-body">
             
            </div>

            <div class="modal-footer">
                
            </div>
        </div>
    </div>
</div>




<div class="panel panel-default">
    <div class="panel-heading">
        <h3>User Table</h3>
    </div>
    <div class="panel-body">
        <button class="btn btn-primary btn-sm" id="registerNewUser">Create User</button>
        <br/>
        <br/>

        <div id="AccountUsertableDiv">
            <table class="table table-hover" id="UserTable">
                <thead>
                    <tr>
                        <th>
                            @Html.DisplayName("CompanyId")
                        </th>
                        <th>
                            @Html.DisplayName("Firstname")
                        </th>
                        <th>
                            @Html.DisplayName("Middlename")
                        </th>
                        <th>
                            @Html.DisplayName("Surname")
                        </th>
                        <th>
                            @Html.DisplayName("Email")
                        </th>
                    </tr>
                </thead>
                <tbody>
                    @foreach (var item in Model)
                    {
                        <tr id="@item.Id">


                            <td id="Username">@Html.DisplayFor(modelItem => item.CompEmployeeNum)</td>

                            <td>
                                @Html.DisplayFor(modelItem => item.Firstname)
                            </td>
                            <td>
                                @Html.DisplayFor(modelItem => item.Middlename)
                            </td>
                            <td>
                                @Html.DisplayFor(modelItem => item.Surname)
                            </td>
                            <td>
                                @Html.DisplayFor(modelItem => item.EmailAddress)
                            </td>
                        </tr>
                    }

                </tbody>

            </table>
        </div>
    </div>
    <div class="panel-footer">

    </div>

</div>


